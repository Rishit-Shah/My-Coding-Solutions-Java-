class Solution {
    public int maxProfit(int prices[]) {

        int minPrice = prices[0];
       
        int x=0;
        

        for (int i = 1; i < prices.length; i++) {
            int current= prices[i];
            minPrice= Math.min(minPrice, current);
             x= Math.max(x, current-minPrice);
         
           
        }

        return x;
    }
}
