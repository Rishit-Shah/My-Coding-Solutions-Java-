class Solution {
    public int missingNumber(int[] nums) {
    Set<Integer> set= new HashSet<>();
    for(int c: nums){
        set.add(c);

    }
    int m= nums.length;
    for(int i=0;i<=m;i++){
        if(!set.contains(i)){
            return i;

        }

    }
    return -1;
        
    }
}
